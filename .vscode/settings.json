{
  "files.associations": {
    "*.repos": "yaml",
    "*.world": "xml",
    "*.xacro": "xml",
    "string": "cpp",
    "type_traits": "cpp"
  },
  "python.analysis.extraPaths": [
    "${workspaceFolder}/**",
  ],
  "python.autoComplete.extraPaths": [
    "${workspaceFolder}/**",
  ],
  "python.envFile": "${workspaceFolder}/.env",
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/build": true,
    "**/install": true,
    "**/log": true
  },
}
